				<title_backend>Kassa Compleet</title_backend>
			</kassa_compleet>
		</df_payment>
        <payment>
            <dfe_kassa_compleet>
				<model>Dfe\KassaCompleet\Method</model>
            </dfe_kassa_compleet>
        </payment>
    </default>
</config>